template kas_crate_cover : staticprop
{
    obinstrenderer render
    {
	    model = "backgrounds/kas/props/kas_crate_cover"
    }

    meta
    {
    	canCreateInEditor  = 1
	    editorInstanceName = "crate_cover"
    	editorPath         = "bf/props/kasshyyyk/cover"
    }
}

