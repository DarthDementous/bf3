// vim: set syntax=c :

autoAimManagerTemplate autoaimmgr
{
    class-id	= "auto aim mgr bf"

    lockedTargetModifier = 0.5f	    // Value between 0.0f and 1.0f. The lower 
				    // it is, the more it favours the currently
				    // locked target.

    autoaimedTargetModifier = 0.5f  // Value between 0.0f and 1.0f. The lower
				    // it is, the more it favours the currently
				    // autoaimed target
}
