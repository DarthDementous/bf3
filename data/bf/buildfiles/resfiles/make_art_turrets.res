// Republic Field Gun
ob_autoLOD @
{
    inputuser = "iharrison"
    inputfile = "bf/props/turrets/rep/rep_fieldgun/rep_fieldgun"
    outputfile = "props/turrets/repfieldgun01"
    extraoptions = "-pnogaplod"
    outputNovodexData =	"true"
    float lods[]
    { 75.0, 35.0, 10.0 }
}

