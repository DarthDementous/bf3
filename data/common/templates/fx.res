// vim: set syntax=c :

template fxcomponent
{
    class-id		    = "fx component"
    electricConductivity    = 0.0f
    electricCharge	    = 0.0f
    flammability	    = 0.0f
    flame		    = 0.0f
    highlight		    = 0

    soundmap-field soundmap
    {
	default = "sndmap_soundmap_fx_def"
    }
}


